#ifndef JESSY_CPPLIB_CSTDDEF
#define JESSY_CPPLIB_CSTDDEF

#include <impl/cstddef.hpp>

namespace std
{

  using impl::cstddef::max_align_t;
  using impl::cstddef::ptrdiff_t;
  using impl::cstddef::size_t;

  using impl::cstddef::nullptr_t;

  using impl::cstddef::byte;

  using impl::cstddef::operator<<;
  using impl::cstddef::operator<<=;
  using impl::cstddef::operator>>;
  using impl::cstddef::operator>>=;
  using impl::cstddef::operator|;
  using impl::cstddef::operator|=;
  using impl::cstddef::operator&;
  using impl::cstddef::operator&=;
  using impl::cstddef::operator^;
  using impl::cstddef::operator^=;
  using impl::cstddef::operator~;

  using impl::cstddef::to_integer;

}  // namespace std

#endif