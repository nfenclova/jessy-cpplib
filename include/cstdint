#ifndef JESSY_CPPLIB_CSTDINT
#define JESSY_CPPLIB_CSTDINT

#include <impl/cstdint.hpp>

namespace std
{
  using impl::cstdint::int16_t;
  using impl::cstdint::int32_t;
  using impl::cstdint::int64_t;
  using impl::cstdint::int8_t;

  using impl::cstdint::int_fast16_t;
  using impl::cstdint::int_fast32_t;
  using impl::cstdint::int_fast64_t;
  using impl::cstdint::int_fast8_t;

  using impl::cstdint::int_least16_t;
  using impl::cstdint::int_least32_t;
  using impl::cstdint::int_least64_t;
  using impl::cstdint::int_least8_t;

  using impl::cstdint::intmax_t;

  using impl::cstdint::intptr_t;

  using impl::cstdint::uint16_t;
  using impl::cstdint::uint32_t;
  using impl::cstdint::uint64_t;
  using impl::cstdint::uint8_t;

  using impl::cstdint::uint_fast16_t;
  using impl::cstdint::uint_fast32_t;
  using impl::cstdint::uint_fast64_t;
  using impl::cstdint::uint_fast8_t;

  using impl::cstdint::uint_least16_t;
  using impl::cstdint::uint_least32_t;
  using impl::cstdint::uint_least64_t;
  using impl::cstdint::uint_least8_t;

  using impl::cstdint::uintmax_t;

  using impl::cstdint::uintptr_t;
}  // namespace std

#endif
